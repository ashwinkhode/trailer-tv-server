{
  "name": "trailer-tv-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "watch": "tsc -w",
    "build2": "npm-run-all clean runtsc",
    "runtsc": "tsc",
    "build": "tsc",
    "clean": "rm -rf dist/*",
    "dbseed": "node dist/utils/seedData.js",
    "dev": "nodemon dist/index.js",
    "migrate:create": "npm run typeorm migration:create -n PostRefactoring",
    "migrate:run": "npm run typeorm migration:run",
    "postbuild": "npm-run-all migrate:create migrate:run dbseed"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.10",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.26.4",
    "@types/node": "^15.12.2",
    "@types/nodemailer": "^6.4.2",
    "@types/redis": "^2.8.29",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "apollo-server-express": "^2.25.1",
    "argon2": "^0.28.2",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql": "^15.5.0",
    "ioredis": "^4.27.5",
    "nodemailer": "^6.6.1",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0"
  },
  "env": {
    "production": "true",
    "verboseApiCalls": "false"
  }
}
